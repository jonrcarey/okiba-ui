{% set title = 'Okiba | Scroll Container' %}
{% set pageCtrl = 'scroll-full-page' %}

{% extends 'templates/_default.njk' %}

{% block main %}
  <div id="scrollable">
    <div class="js-scroll-container">
      <div class="js-scroll-content js-scroll-element">
        <h1>Scroll translation applied on unique content wrapper</h1>
        {% for index in range(0, 100) %}
          <div>
            <h2>Element #{{ index + 1 }}</h2>
          </div>
        {% endfor %}
      </div>
    </div>
  </div>
{% endblock %}

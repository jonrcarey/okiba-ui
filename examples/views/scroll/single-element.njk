{% set title = 'Okiba | Scroll Element' %}
{% set pageCtrl = 'scroll-single-element' %}

{% extends 'templates/_default.njk' %}

{% block main %}
  <div class="js-scroll-container">
    <div class="js-scroll-content">
      <h1 class="js-scroll-element">Scroll translation applied on single elements</h1>
      {% for index in range(0, 100) %}
        <div class="js-scroll-element">
          <h2>Element #{{ index + 1 }}</h2>
        </div>
      {% endfor %}
    </div>
  </div>
{% endblock %}

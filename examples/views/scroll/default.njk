{% set title = 'Okiba | Default Scroll' %}
{% set pageCtrl = 'scroll-default' %}

{% extends 'templates/_default.njk' %}

{% block main %}
  <div id="scrollable">
    <div class="js-scroll-container">
      <div class="js-scroll-content">
        <h1 class="js-scroll-element">Default scroll (native)</h1>
        {% for index in range(0, 100) %}
          <div class="js-scroll-element">
            <h2>Element #{{ index + 1 }}</h2>
          </div>
        {% endfor %}
      </div>
    </div>
  </div>
{% endblock %}
